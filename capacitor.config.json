{
  "appId": "com.hireseat.app",
  "appName": "hire-seat",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    }
  },
  "cordova": {},
  "server": {
    "hostname": "hireseat",
    "iosScheme": "ionic",
    "androidScheme": "https",
    "allowNavigation": [
      "https://hireseat.com:5000"
    ]
  }
}
